<scroll-view class="sell-first-scroll-view">
    <view class="top-div">
        <view class="top-div-top-img">
            <image class="image1" src="http://www.gether.cn/zhuanzhuan/image/sellFirstImg/top-top-image.png"></image>
        </view>
        <view class="top-div-bottom-img">
            <image class="image2" src="http://www.gether.cn/zhuanzhuan/image/sellFirstImg/top-bottom-image.png"></image>
        </view>
    </view>

    <view class="center-div">
        <view class="center-div-inside">
            <view class="top-topic">
                <image src="http://www.gether.cn/zhuanzhuan/image/sellFirstImg/background.png" class="top-topic-image"></image>
                <text class="top-topic-text">这些都可以卖</text>
            </view>
            <view class="bottom-img">
                <view class="img-cover" wx:for="{{sellFirstDivs}}" wx:key="key" wx:for-item="sellFirstDiv">
                    <image src="{{sellFirstDiv.Simg}}" class="img"></image>
                    <view class="img-text">{{sellFirstDiv.Stext}}</view>
                </view>
            </view>
        </view>
    </view>

    <view class="bottom-div">
        <view class="bottom-div-inside">
            <view class="top-topic">
                <image src="http://www.gether.cn/zhuanzhuan/image/sellFirstImg/background.png" class="top-topic-image"></image>
                <text class="top-topic-text">活动规则</text>
            </view>
            <view class="bottom-text">
                <view class="text-content" wx:for="{{sellFirstDiv2s}}" wx:key="key" wx:for-item="sellFirstDiv2">
                    {{sellFirstDiv2.text}}
                </view>
            </view>
        </view>
    </view>
    
    <view class="btn">
        <view class="btn-inside">发布闲置物品</view>
    </view>
</scroll-view>